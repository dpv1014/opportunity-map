<div class="d-flex justify-content-between">
  <h3>Ver Seguimiento Indicador</h3>
  <div>
    <% if can? :index, IndicatorTracking %>
      <%= link_to indicator_indicator_trackings_path(@indicator), class: "btn btn-primary" do %>
        <i class="fas fa-arrow-left"></i> Regresar
      <% end %>
    <% end %>
    <% if can? :edit, @indicator_tracking %>
      <%= link_to edit_indicator_indicator_tracking_path(@indicator, @indicator_tracking), class: "btn btn-warning" do %>
        <i class="fas fa-pencil"></i> Editar
      <% end %>
    <% end %>
    <% if can? :destroy, @indicator_tracking %>
      <%= link_to indicator_indicator_tracking_path(@indicator), method: :delete, data: { confirm: 'Esta seguro?' }, class: "btn btn-danger" do %>
        <i class="fas fa-trash"></i> Eliminar
      <% end %>
    <% end %>
  </div>
</div>

<div class="mt-4">
  <%= render "form", indicator_tracking: @indicator_tracking, disabled: true %>
</div>
